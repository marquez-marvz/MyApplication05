<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    '


    <EditText
        android:id="@+id/txtActivityCode"
        android:layout_width="80dp"
        android:layout_height="60dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="5dp"
        android:enabled="false"
        android:text="ACT-00" />

    <EditText
        android:id="@+id/txtDescription"
        android:layout_width="300dp"
        android:layout_height="60dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="120dp"
        android:layout_marginTop="5dp"
        android:enabled="false"
        android:text="Description" />


    <TextView
        android:id="@+id/txtSheetNameLabel"
        android:layout_width="375dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="75dp"
        android:hint="SHeet Name"
        android:enabled="false"
        android:textSize="18sp" />


    <EditText
        android:id="@+id/txtSheetName"
        android:layout_width="375dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="150dp"
        android:layout_marginTop="75dp"
        android:hint="Import20"
        android:textSize="16sp" />




    <Button
        android:id="@+id/rowBtnSection"
        android:layout_width="75dp"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="150dp"
        android:text="SECTIONE"
        android:textSize="12sp"
        android:visibility="visible" />

    <Button
        android:id="@+id/rowBtnGeneral"
        android:layout_width="75dp"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="110dp"
        android:text="GENERAL"
        android:textSize="12sp"
        android:visibility="visible" />


    <Spinner
        android:id="@+id/cboSheetList"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="100dp"
        android:layout_marginTop="140dp"
        android:spinnerMode="dialog" />

    <Spinner
        android:id="@+id/cboWorkBookList"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="100dp"
        android:layout_marginTop="280dp"
        android:spinnerMode="dialog" />

    <Button
        android:id="@+id/rowBtnSaveSheet"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="200dp"
        android:text="EDIT DATA"
        android:textSize="10sp"
        android:visibility="visible" />

    <Button
        android:id="@+id/rowBtnCSPCImport"
        android:layout_width="60dp"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="80dp"
        android:layout_marginTop="200dp"
        android:text="CSPC Import"
        android:textSize="8sp"
        android:visibility="visible" />
''
    <Button
        android:id="@+id/rowBtnImportStatus"
        android:layout_width="75dp"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="150dp"
        android:layout_marginTop="200dp"
        android:text="IMPORT STATUS"
        android:textSize="10sp"
        android:visibility="visible" />


    <Button
        android:id="@+id/rowBtnImportScore"
        android:layout_width="75dp"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="220dp"
        android:layout_marginTop="200dp"
        android:text="IMPORT SCORE"
        android:textSize="12sp"
        android:visibility="visible" />












</RelativeLayout>